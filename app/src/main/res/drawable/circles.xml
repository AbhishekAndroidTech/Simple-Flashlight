<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="60dp"
        android:height="60dp"
        android:left="60dp"
        android:top="60dp">
        <shape
            android:shape="oval">
            <stroke
                android:width="15dp"
                android:color="@android:color/white"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

    <item
        android:width="120dp"
        android:height="120dp"
        android:left="30dp"
        android:top="30dp">
        <shape
            android:shape="oval">
            <stroke
                android:width="15dp"
                android:color="@android:color/white"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

    <item
        android:width="180dp"
        android:height="180dp">
        <shape
            android:shape="oval">
            <stroke
                android:width="15dp"
                android:color="@android:color/white"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

</layer-list>
