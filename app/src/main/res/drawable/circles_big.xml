        android:bottom="60dp"
        android:left="60dp"
        android:right="60dp"
        android:top="60dp">
        <shape
            android:shape="oval">
            <stroke
                android:width="15dp"
                android:color="@color/translucent_white"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

    <item
        android:bottom="30dp"
        android:left="30dp"
        android:right="30dp"
        android:top="30dp">
        <shape
            android:shape="oval">
            <stroke
                android:width="15dp"
                android:color="@color/translucent_white"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

    <item>
        <shape
            android:shape="oval">
            <stroke
                android:width="15dp"
                android:color="@color/translucent_white"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

</layer-list>
